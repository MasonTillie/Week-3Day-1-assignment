   "source": [
    "import re\n",
    "with open ('regex_test.txt') as f:\n",
    "    data = f.readlines()\n",
    "\n",
    "\n",
    "pattern_name = re.compile(r'([A-Z][a-zA-Z]+).+([A-Z][a-z]+$)')\n",
    "for name in data:\n",
    "    match = pattern_name.search(name)\n",
    "    if match:\n",
    "        print(match.string)\n",
    "    else:\n",
    "        print('None')\n"
   ]
  }
